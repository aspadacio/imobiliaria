	<queryString>
		<![CDATA[SELECT
     tb_pessoa_juridica.`NU_CNPJ` AS pj_cnpj,
     tb_pessoa_juridica.`NO_RAZAO_SOCIAL` AS tb_pessoa_juridica_NO_RAZAO_SOCIAL,
     tb_pessoa_juridica.`NO_FANTASIA` AS tb_pessoa_juridica_NO_FANTASIA,
     tb_pessoa_juridica.`NU_INSCRICAO_ESTADUAL` AS tb_pessoa_juridica_NU_INSCRICAO_ESTADUAL,
     tb_pessoa_juridica.`NO_CONTATO` AS tb_pessoa_juridica_NO_CONTATO,
     tb_pessoa_fisica.`NU_CPF` AS tb_pessoa_fisica_NU_CPF,
     tb_pessoa_fisica.`NO_PESSOA_FISICA` AS tb_pessoa_fisica_NO_PESSOA_FISICA,
     tb_pessoa.`ID_PESSOA` AS tb_pessoa_ID_PESSOA,
     tb_pessoa.`NU_CNPJ` AS tb_pessoa_NU_CNPJ,
     tb_pessoa.`NU_CPF` AS tb_pessoa_NU_CPF,
     tb_pessoa.`DS_EMAIL` AS tb_pessoa_DS_EMAIL,
     tb_pessoa.`DT_ULTIMA_ALTERACAO` AS tb_pessoa_DT_ULTIMA_ALTERACAO,
     tb_pessoa.`DS_OBSERVACAO` AS tb_pessoa_DS_OBSERVACAO,
     tb_municipio.`ID_MUNICIPIO` AS tb_municipio_ID_MUNICIPIO,
     tb_municipio.`NO_MUNICIPIO` AS tb_municipio_NO_MUNICIPIO,
     tb_municipio.`SG_UF` AS tb_municipio_SG_UF,
     tb_locatario.`ID_LOCATARIO` AS tb_locatario_ID_LOCATARIO,
     tb_locatario.`ID_PESSOA` AS tb_locatario_ID_PESSOA,
     tb_locatario.`DT_CADASTRO` AS tb_locatario_DT_CADASTRO,
     tb_locador.`ID_LOCADOR` AS tb_locador_ID_LOCADOR,
     tb_locador.`ID_PESSOA` AS tb_locador_ID_PESSOA,
     tb_locador.`DT_CADASTRO` AS tb_locador_DT_CADASTRO,
     tb_imovel.`ID_IMOVEL` AS tb_imovel_ID_IMOVEL,
     tb_imovel.`DS_COMPLEMENTO` AS tb_imovel_DS_COMPLEMENTO,
     tb_imovel.`DS_ENDERECO` AS tb_imovel_DS_ENDERECO,
     tb_imovel.`DS_IMOVEL` AS tb_imovel_DS_IMOVEL,
     tb_imovel.`DS_OBSERVACOES` AS tb_imovel_DS_OBSERVACOES,
     tb_imovel.`NU_CEP` AS tb_imovel_NU_CEP,
     tb_imovel.`NU_ENDERECO` AS tb_imovel_NU_ENDERECO,
     tb_imovel.`ID_BAIRRO` AS tb_imovel_ID_BAIRRO,
     tb_imovel.`ID_LOCATARIO` AS tb_imovel_ID_LOCATARIO,
     tb_imovel.`ID_MUNICIPIO` AS tb_imovel_ID_MUNICIPIO,
     tb_endereco_pessoa.`ID_ENDERECO_PESSOA` AS tb_endereco_pessoa_ID_ENDERECO_PESSOA,
     tb_endereco_pessoa.`ID_PESSOA` AS tb_endereco_pessoa_ID_PESSOA,
     tb_endereco_pessoa.`ID_MUNICIPIO` AS tb_endereco_pessoa_ID_MUNICIPIO,
     tb_endereco_pessoa.`ID_BAIRRO` AS tb_endereco_pessoa_ID_BAIRRO,
     tb_endereco_pessoa.`NU_CEP` AS tb_endereco_pessoa_NU_CEP,
     tb_endereco_pessoa.`DS_ENDERECO` AS tb_endereco_pessoa_DS_ENDERECO,
     tb_endereco_pessoa.`NU_ENDERECO` AS tb_endereco_pessoa_NU_ENDERECO,
     tb_endereco_pessoa.`DS_COMPLEMENTO` AS tb_endereco_pessoa_DS_COMPLEMENTO,
     tb_endereco_pessoa.`NU_DDD` AS tb_endereco_pessoa_NU_DDD,
     tb_endereco_pessoa.`NU_TELEFONE` AS tb_endereco_pessoa_NU_TELEFONE,
     tb_endereco_pessoa.`TP_ENDERECO` AS tb_endereco_pessoa_TP_ENDERECO,
     tb_contrato.`ID_CONTRATO` AS tb_contrato_ID_CONTRATO,
     tb_contrato.`ID_LOCATARIO` AS tb_contrato_ID_LOCATARIO,
     tb_contrato.`ID_LOCADOR` AS tb_contrato_ID_LOCADOR,
     tb_contrato.`ID_PESSOA_FIADOR` AS tb_contrato_ID_PESSOA_FIADOR,
     tb_contrato.`DT_INICIO` AS tb_contrato_DT_INICIO,
     tb_contrato.`NU_DURACAO` AS tb_contrato_NU_DURACAO,
     tb_contrato.`NU_DIA_VENCIMENTO` AS tb_contrato_NU_DIA_VENCIMENTO,
     tb_contrato.`TX_MULTA_POR_ATRASO` AS tb_contrato_TX_MULTA_POR_ATRASO,
     tb_contrato.`NU_PARCELA_ANTERIOR` AS tb_contrato_NU_PARCELA_ANTERIOR,
     tb_contrato.`TX_COMISSAO` AS tb_contrato_TX_COMISSAO,
     tb_contrato.`ST_CONTRATO_ATIVO` AS tb_contrato_ST_CONTRATO_ATIVO,
     tb_bairro.`ID_BAIRRO` AS tb_bairro_ID_BAIRRO,
     tb_bairro.`ID_MUNICIPIO` AS tb_bairro_ID_MUNICIPIO,
     tb_bairro.`NO_BAIRRO` AS tb_bairro_NO_BAIRRO
FROM
     `tb_pessoa_fisica` tb_pessoa_fisica INNER JOIN `tb_pessoa` tb_pessoa ON tb_pessoa_fisica.`NU_CPF` = tb_pessoa.`NU_CPF`
     INNER JOIN `tb_pessoa_juridica` tb_pessoa_juridica ON tb_pessoa.`NU_CNPJ` = tb_pessoa_juridica.`NU_CNPJ`
     INNER JOIN `tb_locatario` tb_locatario ON tb_pessoa.`ID_PESSOA` = tb_locatario.`ID_PESSOA`
     INNER JOIN `tb_locador` tb_locador ON tb_pessoa.`ID_PESSOA` = tb_locador.`ID_PESSOA`
     INNER JOIN `tb_endereco_pessoa` tb_endereco_pessoa ON tb_pessoa.`ID_PESSOA` = tb_endereco_pessoa.`ID_PESSOA`
     INNER JOIN `tb_contrato` tb_contrato ON tb_pessoa.`ID_PESSOA` = tb_contrato.`ID_PESSOA_FIADOR`
     AND tb_locador.`ID_LOCADOR` = tb_contrato.`ID_LOCADOR`
     AND tb_locatario.`ID_LOCATARIO` = tb_contrato.`ID_LOCATARIO`
     INNER JOIN `tb_imovel` tb_imovel ON tb_locatario.`ID_LOCATARIO` = tb_imovel.`ID_LOCATARIO`
     INNER JOIN `tb_municipio` tb_municipio ON tb_imovel.`ID_MUNICIPIO` = tb_municipio.`ID_MUNICIPIO`
     INNER JOIN `tb_bairro` tb_bairro ON tb_imovel.`ID_BAIRRO` = tb_bairro.`ID_BAIRRO`
     AND tb_municipio.`ID_MUNICIPIO` = tb_bairro.`ID_MUNICIPIO`
     AND tb_bairro.`ID_BAIRRO` = tb_endereco_pessoa.`ID_BAIRRO`
     AND tb_municipio.`ID_MUNICIPIO` = tb_endereco_pessoa.`ID_MUNICIPIO`]]>
	</queryString>
	<title>
		<band height="35" splitType="Stretch">
